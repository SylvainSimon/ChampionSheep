{
    "name": "SylvainSimon/ChampionSheep",
    "type": "project",
    "autoload": {
        "psr-4": {
            "": ["src", "library/Common", "library"],
            "Entity\\": "library/Entity",
            "Base\\": "library/Base",
            "Repository\\": "library/Repository",
            "CoreBundle\\Controllers\\": "src/CoreBundle/Controllers",
            "CoreBundle\\Forms\\": "src/CoreBundle/Forms",
            "CoreBundle\\Emails\\": "src/CoreBundle/Emails",
            "CoreBundle\\Forms\\Validators\\": "src/CoreBundle/Forms/Validators",
            "CoreBundle\\TwigExtension\\": "src/CoreBundle/TwigExtension"
        }
    },
    "config": {
        "component-dir": "web/public/components",
        "component-baseurl": "web/public/components"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/SylvainSimon/Libraries"
        }
    ],
    "http-basic": {
        "github.com": {
            "username": "SylvainSimon"
        }
    },
    "require": {
        "php": "~5.6",
        "monolog/monolog": "1.21.*",
        "SylvainSimon/Libraries": "dev-master",
        "robloach/component-installer": "0.2.*",
        "components/normalize.css": "3.0.*",
        "components/bootstrap": "3.3.*",
        "components/jquery": "3.1.*",
        "components/jqueryui": "1.12.*",
        "symfony/form": "3.1.*",
        "symfony/twig-bridge": "3.1.*" ,
        "symfony/validator": "3.1.*" ,
        "symfony/security": "3.1.*" ,
        "symfony/translation": "3.1.*",
        "symfony/options-resolver": "3.1.*",
        "tijsverkoyen/css-to-inline-styles": "2.2.*"
    }
}
